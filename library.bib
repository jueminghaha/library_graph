@misc{Suprijanto2010,
   abstract = {A finite set $X \seq \RRn$ with a weight function $w : X \longrightarrow \RR_\\{\{\textgreater\}0\}$ is called \emph\{Euclidean $t$-design\} in \$\{\textbackslash\}RRn$ (supported by $p$ concentric spheres) if the following condition holds: \[ \sum_\{i=1\\}\{\textasciicircum\}p \frac\{w(X_i)\}\\{\{\textbar\}S_i\\}\{\textbackslash\}int_\{S_i\} f(\boldsymbol x)d\sigma_i(\boldsymbol x) =\sum_\\{\{\textbackslash\}boldsymbol x \in X\}w(\boldsymbol x) f(\boldsymbol x), \] for any polynomial $f(\boldsymbol x) \in \mbox\{Pol\}(\RRn)$ of degree at most $t$. Here $S_i \seq \RRn$ is a sphere of radius $r_i \geq 0,$ $X_i=X \cap S_i,$ and \$\{\textbackslash\}sigma_i(\boldsymbol x)$ is an $O(n)$-invariant measure on $S_i$ such that \$\{\textbar\}S_i=r_i\{n-1\\}\{\textbar\}S\{n-1\\}\{\textbar\}$, with \$\{\textbar\}S_i$ is the surface area of $S_i$ and \$\{\textbar\}S\{n-1\\}\{\textbar\}$ is a surface area of the unit sphere in \$\{\textbackslash\}RRn$. Recently, Bajnok (2006) constructed tight Euclidean $t$-designs in the plane ($n=2$) for arbitrary $t$ and $p.$ In this paper we show that for case $t=6$ and $p=2,$ tight Euclidean $6$-designs constructed by Bajnok is the unique configuration in \$\{\textbackslash\}RRn$, for $2 \leq n \leq 8.$},
   author = {Djoko Suprijanto},
   keywords = {Mathematics - Combinatorics},
   month = {12},
   note = {arXiv:1012.1946},
   publisher = {arXiv},
   title = {On tight Euclidean $6$-designs: an experimental result},
