@misc{Kodalen2017,
   abstract = {Let $(X,\mathcal\{R\})$ be a commutative association scheme and let \$\{\textbackslash\}Gamma=(X,R\cup R\top)$ be a connected undirected graph where $R\in \mathcal\{R\}$. Godsil (resp., Brouwer) conjectured that the edge connectivity (resp., vertex connectivity) of \$\{\textbackslash\}Gamma$ is equal to its valency. In this paper, we prove that the deletion of the neighborhood of any vertex leaves behind at most one non-singleton component. Two vertices $a,b\in X$ are called "twins" in \$\{\textbackslash\}Gamma$ if they have identical neighborhoods: \$\{\textbackslash\}Gamma(a)=\Gamma(b)$. We characterize twins in polynomial association schemes and show that, in the absence of twins, the deletion of any vertex and its neighbors in \$\{\textbackslash\}Gamma$ results in a connected graph. Using this and other tools, we find lower bounds on the connectivity of \$\{\textbackslash\}Gamma$, especially in the case where \$\{\textbackslash\}Gamma$ has diameter two. Among the applications of these results, we find that the only connected relations in symmetric association schemes which admit a disconnecting set of size two are those which are ordinary polygons.},
   author = {Brian G Kodalen and William J Martin},
   keywords = {Mathematics - Combinatorics},
   month = {9},
   note = {arXiv:1702.03801},
   publisher = {arXiv},
   title = {On the connectivity of graphs in association schemes},
   url = {http://arxiv.org/abs/1702.03801},
   year = {2017},
}
@misc{Musin2006,
   abstract = {The kissing number problem asks for the maximal number k(n) of equal size nonoverlapping spheres in n-dimensional space that can touch another sphere of the same size. This problem in dimension three was the subject of a famous discussion between Isaac Newton and David Gregory in 1694. In three dimensions the problem was finally solved only in 1953 by Sch\"utte and van der Waerden. In this paper we present a solution of a long-standing problem about the kissing number in four dimensions. Namely, the equality k(4)=24 is proved. The proof is based on a modification of Delsarte's method.},
   author = {Oleg R Musin},
